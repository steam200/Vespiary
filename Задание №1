  #include <iostream>
#include <cstring>
using namespace std;
 
int main ()
{
	int counter=0;
	char text[100];
	char vowel[] = "AEOUIYaeouiy";
	char * pch;
	cin.getline(text,100);
	pch = strpbrk (text, vowel);
	while (pch != NULL) // пока есть гласные буквы
	{
		pch = strpbrk (pch+1,vowel); // поиск гласных букв
		counter++;
	}
	cout << counter;
	return 0;
}
